{
  "name": "timeforing",
  "version": "1.0.0",
  "description": "Time Tracking Web Application with Vue.js frontend and Node.js backend",
  "main": "index.js",
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "start:backend": "cd backend && npm run dev",
    "start:frontend": "cd frontend && npm run dev",
    "start:both": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "build": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "kill-servers": "kill-servers.bat"
  },
  "keywords": ["timetracking", "vue", "express", "typescript"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}